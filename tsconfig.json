{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6"],
    "module": "es6",
    "rootDir": "./src",
    "moduleResolution": "node",
    "incremental": true,
    "types": ["node"],
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
